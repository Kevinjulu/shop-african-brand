{
  "name": "shop-african-brand",
  "build": {
    "command": "npm install && npm run build",
    "directory": "dist",
    "environment": {
      "NODE_VERSION": "18.18.0",
      "NPM_VERSION": "9.6.7"
    }
  },
  "routes": [
    {
      "pattern": "/*",
      "dest": "/index.html",
      "status": 200
    }
  ]
}
